{
  "name": "wuffle",
  "version": "0.34.0",
  "description": "The wuffle board back-end",
  "author": {
    "name": "Nico Rehwaldt",
    "url": "https://github.com/nikku"
  },
  "bin": {
    "wuffle": "./bin/wuffle"
  },
  "license": "MIT",
  "homepage": "https://github.com/nikku/wuffle",
  "repository": {
    "type": "git",
    "url": "https://github.com/nikku/wuffle.git",
    "directory": "packages/app"
  },
  "keywords": [
    "probot",
    "github",
    "probot-app",
    "github issues",
    "task board",
    "kanban board",
    "project management"
  ],
  "scripts": {
    "all": "run-s lint lint:types test",
    "dev": "nodemon",
    "start": "node ./bin/run.js",
    "test": "mocha test --exit",
    "lint": "run-s lint:*",
    "lint:eslint": "eslint .",
    "lint:types": "tsc --pretty",
    "auto-test": "npm test -- --watch"
  },
  "dependencies": {
    "async-didi": "^0.2.1",
    "aws-sdk": "^2.786.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "exit-hook2": "^1.0.8",
    "express-session": "^1.17.1",
    "min-dash": "^3.5.2",
    "p-defer": "^3.0.0",
    "probot": "^9.15.1"
  },
  "devDependencies": {
    "@types/express-session": "^1.17.2",
    "@types/node-fetch": "^2.5.7",
    "chai": "^4.2.0",
    "eslint": "^7.12.1",
    "eslint-plugin-bpmn-io": "^0.11.0",
    "mkdirp": "^0.5.1",
    "mocha": "^6.2.3",
    "nock": "^10.0.0",
    "nodemon": "^1.19.4",
    "npm-run-all": "^4.1.5",
    "sinon": "^7.5.0",
    "sinon-chai": "^3.5.0",
    "smee-client": "^1.2.2",
    "typescript": "^4.0.5"
  },
  "engines": {
    "node": ">= 10.0"
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ],
    "ignore": [
      "public/*",
      "tmp/*",
      "test/*"
    ]
  }
}
